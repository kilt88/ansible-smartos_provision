{
  "remoteVMs": [
{% for host in remote_vms if item.item != host.hypervisor_host %}
    {
      "uuid": "{{ host.uuid }}",
      "owner_uuid": "{{ host.owner_uuid }}",
      "ips": [
{%   for ip in host.ips | split(' ') %}
                "{{ ip }}"{% if not loop.last %},
{%                           endif %}
{%   endfor %}
      ],
      "tags": { "role": "{{ host.role }}" }
    }{% if not loop.last %},
{%      endif %}
{% endfor %}

  ]
}
